PUBLIC_PATH="https://client.website.com/appPath/"

# in bytes (10MB)
NUXT_PUBLIC_UPLOAD_SIZE_LIMIT=10000000
NUXT_PUBLIC_FILES_PER_UPLOAD_REQUEST=5

# Server that is running your analytic software
NUXT_PUBLIC_ANALYTICS_URL="https://client.website.com/b/"