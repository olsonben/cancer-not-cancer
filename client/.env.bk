# Port to be used during development
PORT=3000

# http for local, https for deployment
PROTOCOL=https

# backend api url
NUXT_PUBLIC_API_URL='https://api.website.com/pathToAPI/'

# needed for absolute urls(for CDNs) and deriving app pathname
